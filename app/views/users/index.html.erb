<%= render 'books/book', user: @user, book: @book %>
<h1>Users</h1>
<table>
  <thead>
    <th>image</th>
    <th>name</th>
  </thead>
  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td>
        <%= image_tag user.get_profile_image(100, 100) %>
      </td>
      <td>
        <%= user.name %>
      </td>
      <td>
        <%= link_to "Show", user_path(user.id) %>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>